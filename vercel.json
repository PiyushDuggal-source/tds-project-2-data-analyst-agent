{
  "version": 2,
  "builds": [
    {
      "src": "tds_project.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "tds_project.py"
    }
  ],
  "build": {
    "env": {
      "PIP_INSTALLER": "uv"
    }
  },
  "installCommand": "uv sync"
}